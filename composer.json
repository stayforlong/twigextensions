{
    "name": "stayforlong/twigextensions",
    "description": "Twig extensions to use in Laravel > 5.*",
    "license": "MIT",
    "authors": [
        {
            "name": "Raúl Morón",
            "email": "raul@stayforlong.com"
        }
    ],
    "require": {
        "php": ">=7",
        "ext-intl": "*",
        "stayforlong/sentry": "^6",
        "stayforlong/twigstay": "0.14.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/stayforlong/sentry"
        }
    ],
    "autoload": {
        "psr-4": {
            "StayForLong\\TwigExtensions\\": "src/"
        }
    },
    "branch-alias": {
        "dev-master": "dev"
    },
    "config": {
        "optimize-autoloader": true
    }
}
